    "values": [
        "\u8d8a\u72f1\u5154 \u7b2c\u4e09\u5b63",
        "\u65e5\u672c",
        "\u5bcc\u5ca1\u8061",
        "\u5bcc\u5ca1\u8061",
        "\u559c\u5267\uff0c\u52a8\u753b\uff0c\u77ed\u7247",
        "\u4e0a\u91ce\u5927\u5178",
        "\u65e5\u8bed"
    ],
    "para": "\u300a\u8d8a\u72f1\u5154\u300b\u7cfb\u5217\u52a8\u753b\uff0c\u4e3b\u8981\u56f4\u7ed5\u4e24\u53ea\u57281961\u5e74\u82cf\u8054\u65f6\u4e8e\u76d1\u72f1\u670d\u5211\u7684\u5154\u5b50\u666e\u4eac\u548c\u57fa\u91cc\u8fde\u79d1\u5c55\u5f00\uff0c\u8bb2\u8ff0\u53d1\u751f\u5728\u4ed6\u4eec\u8eab\u4e0a\u98ce\u8da3\u641e\u7b11\u7684\u559c\u5267\u6545\u4e8b\u3002\u7eff\u8272\u56da\u8863\u7684\u666e\u4eac\uff0c\u672c\u6765\u662f\u5de5\u4eba\uff0c\u56e0\u4e3a\u559d\u9189\u9152\u4e0a\u73ed\u6253\u778c\u7761\u88ab\u6293\u5230\u72f1\u91cc\uff0c\u662f\u4e00\u53ea\u6781\u4e3a\u4e50\u5929\u6d3e\u7684\u5154\u5b50\uff0c\u4e2a\u6027\u548c\u5584\u5929\u7136\u5446\u3002\u7ea2\u8272\u56da\u8863\u7684\u57fa\u91cc\u8fde\u79d1\u662f\u524d\u9ed1\u624b\u515a\u8001\u5927\uff0c\u6b66\u529f\u9ad8\u5f3a\uff0c\u88ab\u624b\u4e0b\u80cc\u53db\u70b8\u70c2\u540e\u63a5\u5408\u56de\u53bb\uff0c\u8eab\u4e0a\u6709\u8bb8\u591a\u63a5\u75d5\u53ca\u989c\u8272\u3002\u7231\u5e06\u5e03\u978b\u6210\u75f4\uff0c\u62db\u724c\u52a8\u4f5c\u5c31\u662f\u8eba\u5728\u5404\u79cd\u5730\u65b9\u770b\u7403\u978b\u6742\u5fd7\u3002\u7b2c\u4e09\u5b63\u7684\u6545\u4e8b\u91cc\uff0c\u57fa\u91cc\u8fde\u79d1\u548c\u666e\u4eac\u5230\u4e86\u83ab\u65af\u79d1\uff0c\u8fdb\u5165\u4e00\u6240\u5927\u53a6\u6253\u7b97\u8d2d\u4e70\u9650\u91cf\u7248\u5e06\u5e03\u978b\u3002\u800c\u5927\u53a6\u8001\u677f\u5179\u9c81\u5b97\u6d1b\u592b\u662f\u57fa\u91cc\u8fde\u79d1\u4ece\u524d\u7684\u624b\u4e0b\uff0c\u4e24\u4eba\u79ef\u4ec7\u5df2\u4e45\u3002\u8001\u677f\u6253\u7b97\u9664\u6389\u4ed6\u4fe9\uff0c\u53ef\u6700\u7ec8\u57fa\u91cc\u8fde\u79d1\u8fd8\u662f\u987a\u5229\u5730\u62ff\u5230\u4e86\u9650\u91cf\u7248\u5e06\u5e03\u978b\u3002\u98ce\u9761\u7f51\u7edc\u7684\u65e5\u672c\u5361\u901a\u7cfb\u5217\u77ed\u7247\u300a\u8d8a\u72f1\u5154\u300b\uff0c\u6bcf\u5b63\u5171\u5341\u4e09\u96c6\u3001\u6bcf\u96c6\u540490\u79d2\uff0c\u81ea2006\u5e74\u81f3\u4eca\u63a8\u51fa\u4e94\u5b63\uff08\u4ecd\u5728\u66f4\u65b0\u4e2d\uff09\u30022006\u5e74\u5165\u9009\u6587\u5316\u5385\u5a92\u4f53\u827a\u672f\u8282\u77ed\u7bc7\u52a8\u753b\u5956\uff0c2007\u5e74\u5165\u9009\u6e25\u592a\u534e\u56fd\u9645\u52a8\u753b\u7535\u5f71\u8282\u6210\u4eba\u5411\u7535\u89c6\u52a8\u753b\u4e0a\u91ce\u5927\u5178"
}