        "\u5bfc\u00a0\u00a0\u00a0\u00a0\u6f14",
        "\u4e3b\u00a0\u00a0\u00a0\u00a0\u6f14",
        "\u96c6\u00a0\u00a0\u00a0\u00a0\u6570",
        "\u6bcf\u96c6\u957f\u5ea6",
        "\u7c7b\u00a0\u00a0\u00a0\u00a0\u578b",
        "\u5236\u7247\u4eba",
        "\u5728\u7ebf\u64ad\u653e\u5e73\u53f0"
    ],
    "values": [
        "\u4e1c\u65b9\u534e\u5c14\u8857",
        "\u4f01\u9e45\u5f71\u89c6\u3001\u798f\u65af\u4f20\u5a92\u96c6\u56e2",
        "\u4e2d\u56fd",
        "2018\u5e745\u670824\u65e5",
        "\u9ec4\u56fd\u5f3a",
        "\u5434\u9547\u5b87\uff0c\u5f20\u5b5d\u5168\uff0c\u4f59\u7537\uff0c\u9648\u5bb6\u4e50\uff0c\u6c5f\u5947\u9716\uff0c\u8521\u6d01\uff0c\u6f58\u707f\u826f\uff0c\u8c2d\u8000\u6587\uff0c\u5f20\u53ef\u9890",
        "5\u96c6",
        "60\u5206\u949f",
        "\u65f6\u88c5\u5546\u6218\u8ff7\u4f60\u5267",
        "\u674e\u8dc3",
        "\u817e\u8baf\u89c6\u9891"
    ],
    "para": "\u300a\u4e1c\u65b9\u534e\u5c14\u8857\u300b\u662f\u7531\u4f01\u9e45\u5f71\u89c6\u3001\u798f\u65af\u4f20\u5a92\u96c6\u56e2\u51fa\u54c1\u7684\u65f6\u88c5\u5546\u6218\u8ff7\u4f60\u7f51\u7edc\u5267\uff0c\u7531\u5218\u5fb7\u534e\u62c5\u4efb\u76d1\u5236\uff0c\u9ec4\u56fd\u5f3a\u6267\u5bfc\uff0c\u5434\u9547\u5b87\u3001\u5f20\u5b5d\u5168\u3001\u4f59\u7537\u3001\u9648\u5bb6\u4e50\u3001\u6c5f\u5947\u9716\u3001\u8521\u6d01\u3001\u6f58\u707f\u826f\u7b49\u4e3b\u6f14\uff0c\u8c2d\u8000\u6587\u3001\u5f20\u53ef\u9890\u7279\u522b\u51fa\u6f14\u00a0\u3002\u8be5\u5267\u8bb2\u8ff0\u53f6\u62b1\u4e00\u548c\u97e6\u822a\u5e0c\u671b\u7ef4\u62a4\u5065\u5eb7\u91d1\u878d\u79e9\u5e8f\uff0c\u4f46\u5e08\u5f92\u5374\u53cd\u76ee\u6210\u4ec7\uff0c\u5c55\u5f00\u4e86\u4e00\u573a\u4e2a\u4eba\u6069\u6028\u548c\u793e\u4f1a\u8d23\u4efb\u7684\u7cbe\u5f69\u535a\u5f08\uff0c\u5e76\u5c06\u91d1\u878d\u5e02\u573a\u5377\u5165\u4e86\u4e89\u6597\u7684\u6f29\u6da1\u4e2d\u7684\u6545\u4e8b\u3002\u8be5\u7247\u4e8e2018\u5e745\u670824\u65e5\u64ad\u51fa\u3002\u00a0\u53f6\u62b1\u4e00\uff08\u5434\u9547\u5b87\u9970\uff09\u548c\u97e6\u822a\uff08\u5f20\u5b5d\u5168\u9970\uff09\u539f\u672c\u662f\u5e08\u5f92\u5173\u7cfb\uff0c\u4ed6\u4eec\u6000\u63e3\u7406\u60f3\u7ec4\u5efaCASH10\uff0c\u5e0c\u671b\u51ed\u501f\u4e24\u4eba\u4e4b\u529b\u7ef4\u62a4\u5065\u5eb7\u91d1\u878d\u79e9\u5e8f\uff0c\u65e0\u5948\u88ab\u91d1\u878d\u5de8\u9cc4\u5e55\u540e\u64cd\u63a7\uff0c\u5e08\u5f92\u5374\u53cd\u76ee\u6210\u4ec7\uff0c\u5341\u5e74\u540eCASH20\u7c89\u58a8\u767b\u573a\uff0c\u5c55\u5f00\u4e86\u4e00\u573a\u4e2a\u4eba\u6069\u6028\u548c\u793e\u4f1a\u8d23\u4efb\u7684\u7cbe\u5f69\u535a\u5f08\uff0c\u5e76\u5c06\u6574\u4e2a\u91d1\u878d\u5e02\u573a\u5377\u5165\u4e86\u4e89\u6597\u7684\u6f29\u6da1\u4e2d\u00a0\u3002\u00a0\u00a0\u00a0[4-6]\u00a0\u00a0\u8be5\u5267\u5728\u4e1c\u4eac\u548c\u5317\u4eac\u914d\u97f3\uff0c\u5728\u6cf0\u56fd\u6df7\u97f3\uff0c\u4ece\u7b79\u5907\u5230\u64ad\u51fa\u7528\u4e86\u4e09\u5e74\u65f6\u95f4\u00a0\u3002\u5218\u5fb7\u534e\u62c5\u4efb\u4e86\u8be5\u5267\u7684\u76d1\u5236\uff0c\u4f46\u56e0\u4e3a\u6454\u9a6c\u53d7\u4f24\uff0c\u65e0\u6cd5\u5168\u7a0b\u53c2\u4e0e\u62cd\u6444\u00a0\u3002\u5218\u5fb7\u534e\u5728\u5267\u672c\u521b\u4f5c\u671f\u95f4\uff0c\u4ece\u521d\u7a3f\u5230\u5206\u955c\u5168\u90e8\u90fd\u770b\u8fc7\u4e00\u904d\uff0c\u5e76\u5728\u4e24\u5cb8\u4e09\u5730\u6311\u9009\u6f14\u5458\u3002\u00a0\u8be5\u5267\u4ee51997\u5e74\u30012008\u5e74\u30012018\u5e74\u4e09\u6b21\u91d1\u878d\u5371\u673a\u4e3a\u521b\u4f5c\u80cc\u666f\u3002\u4ee5\u4e24\u4f4d\u7537\u4e3b\u53f6\u62b1\u4e00\u548c\u97e6\u822a\u7684\u547d\u8fd0\u4e3a\u5207\u9762\uff0c\u5c55\u73b0\u9999\u6e2f\u91d1\u878d\u5e02\u573a20\u5e74\u7684\u53d1\u5c55\u53d8\u8fc1\u3002\u8be5\u5267\u4ece\u7acb\u9879\u3001\u7b79\u5907\u5267\u672c\u9636\u6bb5\uff0c\u662f\u6309\u7167\u7f8e\u5267\u7684\u6a21\u5f0f\u548c\u6d41\u7a0b\u6765\u8bbe\u8ba1\uff0c\u5728\u62cd\u6444\u8fc7\u7a0b\u4e2d\uff0c\u89c4\u683c\u90fd\u662f\u6309\u7167\u7535\u5f71\u6807\u51c6\u53bb\u62cd\u6444\uff0c\u6444\u5f71\u3001\u706f\u5149\u3001\u9053\u5177\u7b49\u5236\u4f5c\u73ed\u5e95\u4e5f\u591a\u6765\u81ea\u7535\u5f71\u5236\u4f5c\u56e2\u961f\u3002\u00a0\u6b63\u9762\u8bc4\u4ef7\u300a\u4e1c\u65b9\u534e\u5c14\u8857\u300b\u662f\u4e00\u90e8\u8ff7\u4f60\u5267\uff0c\u8be5\u5267\u501f\u9274\u4e86\u5f88\u591a\u7f8e\u5267\u7684\u7ecf\u9a8c\uff0c\u6bd4\u8d77\u8d8a\u6765\u8d8a\u62d6\u6c93\u4e14\u76f4\u63a5\u7684\u7f51\u5267\uff0c\u660e\u663e\u8981\u66f4\u4e3a\u5feb\u6377\u3001\u201c\u70e7\u8111\u201d\u3002\u4e0d\u4f46\u5f00\u7bc7\u5373\u9ad8\u80fd\uff0c\u5267\u60c5\u4e0e\u4eba\u7269\u529f\u80fd\u6307\u5411\u66f4\u662f\u4e00\u73af\u5957\u4e00\u73af\u3002\uff08\u6e1d\u62a5\u8bc4\uff09\u00a0\u4e1c\u65b9\u534e\u5c14\u8857\u5267\u7167(7\u5f20)\u8be5\u5267\u51b7\u8272\u8c03\u7684\u8272\u5f69\u5904\u7406\u5c06\u8be5\u5267\u7684\u91d1\u878d\u80cc\u666f\u70d8\u6258\u5230\u4e86\u6781\u81f4\u3002\u7070\u6697\u7684\u9650\u5236\u8272\u5f69\u6697\u793a\u4e86\u91d1\u878d\u5708\u7684\u51b7\u8840\u4e0e\u65e0\u60c5\uff0c\u4e13\u4e1a\u7684\u753b\u9762\u6784\u56fe\u3001\u6070\u5230\u597d\u5904\u7684\u80cc\u666f\u97f3\u4e50\uff0c\u4e5f\u4e3a\u6574\u4f53\u5267\u60c5\u8d77\u5230\u4e86\u5f88\u597d\u7684\u70d8\u6258\u4f5c\u7528\u3002\uff08\u53c2\u8003\u6d88\u606f\u7f51\u8bc4\uff09\u00a0\u53cd\u9762\u8bc4\u4ef7\u300a\u4e1c\u65b9\u534e\u5c14\u8857\u300b\u6cbe\u67d3\u4e86\u4e2d\u56fd\u5185\u5730\u5267\u7684\u6bdb\u75c5\uff0c\u591a\u95ea\u56de\u867d\u7684\u5267\u60c5\uff0c\u7a0d\u5acc\u505a\u4f5c\u3002\u6781\u5177\u7535\u5f71\u98ce\u683c\u7684\u8d28\u611f\u753b\u9762\u56fa\u7136\u662f\u5347\u7ea7\uff0c\u4f46\u6709\u65f6\u663e\u5f97\u6545\u5f04\u7384\u865a\u3002\u4e8c\u8005\u90fd\u51b2\u6de1\u4e86\u4eba\u6027\u94fa\u9648\u3002\u8ff7\u4f60\u5267\u7684\u957f\u5ea6\u4e5f\u9650\u5236\u4e86\u6e2f\u5267\u7279\u6709\u7684\u201c\u5403\u5403\u559d\u559d\u5ef6\u7eed\u5267\u60c5\u201d\u3002\uff08\u65b0\u4eac\u62a5\u8bc4\uff09\u00a0"
}