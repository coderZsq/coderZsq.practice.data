    "values": [
        "\u9f99\u622c",
        "\u622c\u6bbf (\u7c89\u4e1d\u79f0\u547c)",
        "\u9648\u79e9",
        "\u300a",
        "\u8d85\u517d\u6b66\u88c5\u4e4b\u4ec1\u8005\u65e0\u654c",
        "\u300b",
        "\u300a\u8d85\u517d\u6b66\u88c5\u4e4b\u52c7\u8005\u65e0\u60e7\u300b",
        "17\u5c81\uff08\u6c38\u6052\u8f6e\u56de\uff09",
        "\u7537",
        "\u5251\u9f99",
        "\u6218\u9f99",
        "\u9f99\u83b9"
    ],
    "para": "\u9f99\u622c\uff0c\u5e7f\u5dde\u84dd\u5f27\u6587\u5316\u4f20\u64ad\u6709\u9650\u516c\u53f8\u3001\u5e7f\u4e1c\u5965\u98de\u52a8\u6f2b\u6587\u5316\u80a1\u4efd\u6709\u9650\u516c\u53f8\u5171\u540c\u6295\u8d44\u7684\u56fd\u4ea73D\u957f\u7bc7\u52a8\u6f2b\u300a\u8d85\u517d\u6b66\u88c5\u4e4b\u4ec1\u8005\u65e0\u654c\u300b\u548c\u300a\u8d85\u517d\u6b66\u88c5\u4e4b\u52c7\u8005\u65e0\u60e7\u300b\u300a\u8d85\u517d\u6b66\u88c5\u4e4b\u5343\u9762\u82f1\u96c4\u300b\u4e2d\u7684\u89d2\u8272\u3002\u8d85\u517d\u6218\u961f\u7684\u4e3b\u8981\u6210\u5458\u4e4b\u4e00\u3002\u4ed6\u51b7\u9759\u3001\u6c89\u7740\uff0c\u7edd\u4e0d\u86ee\u5e72\u7684\u6280\u5de7\u578b\u6218\u58eb\uff0c\u62e5\u6709\u8fc7\u4eba\u7684\u667a\u6167\u3002\u5916\u51b7\u5185\u70ed\uff0c\u8868\u9762\u51b7\u6f20\u9ad8\u50b2\u3001\u5185\u5fc3\u5374\u611f\u60c5\u7ec6\u817b\uff0c\u4e0d\u8f7b\u6613\u8868\u8fbe\u81ea\u5df1\u7684\u771f\u5b9e\u60c5\u611f\u3001\u662f\u8d85\u517d\u6218\u961f\u4e2d\u4e0d\u53ef\u6216\u7f3a\u7684\u4e00\u5458\uff0c\u5728\u6218\u6597\u4e2d\u8d77\u7740\u4e0d\u53ef\u6216\u7f3a\u7684\u4f5c\u7528\u3002\u00a0\u9f99\u622c(20\u5f20)\u8eab\u4efd\uff1a\u8d85\u517d\u6218\u961f\u4e03\u4e2a\u6210\u5458\u4e4b\u4e00\u00a0\uff0c\u5251\u9f99\u7684\u5f1f\u5b50\u5e74\u9f84\uff1a17\u5c81\uff08\u6c38\u6052\u8f6e\u56de\uff09\uff1b\u4e0d\u5230\u5341\u4e07\u5c81\u6027\u683c\uff1a\u51b7\u9759\u3001\u6c89\u7740\uff0c\u62e5\u6709\u8fc7\u4eba\u7684\u667a\u6167\u3002\u5916\u51b7\u5185\u70ed\uff0c\u8868\u9762\u51b7\u6f20\u9ad8\u50b2\u3001\u5185\u5fc3\u5374\u611f\u60c5\u7ec6\u817b\uff0c\u4e0d\u8f7b\u6613\u8868\u8fbe\u81ea\u5df1\u7684\u771f\u5b9e\u60c5\u611f\u3001\u662f\u8d85\u517d\u6218\u961f\u4e2d\u4e0d\u53ef\u6216\u7f3a\u7684\u4e00\u5458\uff0c\u5728\u6218\u6597\u4e2d\u8d77\u7740\u4e0d\u53ef\u6216\u7f3a\u7684\u4f5c\u7528\u00a0\u3002\u6b66\u88c5\uff1a\u9f99\u6218\u6b66\u88c5\u5750\u9a91\uff1a\u9f99\u6218\u95ea\u9a70\u8d85\u517d\uff1a\u9f99\u6218\u517d\u2014\u2014\u9f99\u6218\u795e\u7edd\u62db\uff1a\u5bd2\u5f71\u8bc0\u00a0\u8981\u8bc0\uff1a\u653b\u654c\u4e09\u5206\uff0c\u81ea\u7559\u4e03\u5206\u3002\u5148\u5b88\u540e\u653b\u5f02\u80fd\u91cf\uff1a02\u4e2a\u9ed1\u6d1e\uff0c\u7b2c\u4e00\u90e8\u5927\u7ed3\u5c40\u76f4\u5230\u7b2c\u4e8c\u90e8\u7a7f\u8d8a\u56de\u5341\u4e07\u5e74\u524d\u5f02\u80fd\u91cf\u53ef\u8fbe\u523005\u4e2a\u9ed1\u6d1e\u3001\u7a7f\u8d8a\u56de\u5341\u4e07\u5e74\u540e\u5f02\u80fd\u91cf\u53ef\u8fbe\u523008\u4e2a\u9ed1\u6d1e\uff0c\u5927\u7ed3\u5c40\u4e3a35\u4e2a\u9ed1\u6d1e\u5e08\u7236\uff1a\u5251\u9f99\u5e08\u5144\uff1a\u6218\u9f99\u5f52\u5c5e\u5730\uff1a\u7b2c\u4e8c\u5e73\u884c\u5b87\u5b99\u00a0\u51b7\u50b2\u7684\u5916\u8868\u4e0b\u6709\u4e00\u9897\u5584\u826f\u7684\u5fc3\uff0c\u4e0d\u8f7b\u6613\u8868\u8fbe\u81ea\u5df1\u7684\u771f\u5b9e\u60c5\u611f\u3001\u4f46\u662f\u5f88\u5728\u4e4e\u4f19\u4f34\u3002\u5341\u4e07\u5e74\u524d\u4eb2\u773c\u76ee\u7779\u4e86\u9f99\u65cf\u5185\u6218\u7684\u5168\u90e8\u7ecf\u5386\uff0c\u5e76\u7528\u5bd2\u5f71\u8bc0\u6551\u4e0b\u9f99\u98de\u7684\u5973\u513f\u9f99\u83b9\u3002\u4f46\u662f\u5341\u4e07\u5e74\u540e\uff0c\u9f99\u622c\u53c8\u7528\u6551\u8fc7\u5979\u7684\u5bd2\u5f71\u8bc0\u6740\u4e86\u5979\u30021\u653b\u654c\u4e09\u5206\uff0c\u81ea\u7559\u4e03\u5206\u30022\u5931\u8d25\u5e76\u4e0d\u53ef\u6015\uff0c\u653e\u5f03\u624d\u662f\u53ef\u803b\u7684\uff013\u6ca1\u6709\u95ef\u4e0d\u8fc7\u7684\u5173\u53e3\uff0c\u4e5f\u6ca1\u6709\u6253\u4e0d\u5012\u7684\u654c\u4eba\uff014\u6ca1\u6709\u6253\u4e0d\u5012\u7684\u654c\u4eba\uff0c\u4e5f\u6ca1\u6709\u95ef\u4e0d\u8fc7\u7684\u96be\u5173\uff015\u53ea\u6709\u7ecf\u8fc7\u4e03\u91cd\u7684\u5b64\u72ec\uff0c\u624d\u80fd\u6210\u4e3a\u771f\u6b63\u7684\u5f3a\u8005\u30026\u8fd9\u4e16\u4e0a\u5145\u6ee1\u4e86\u7231\uff0c\u53ea\u4e0d\u8fc7\u4f60\u8fd9\u79cd\u4eba\uff0c\u4e0d\u61c2\u5f97\u600e\u6837\u53bb\u7231\u800c\u5df2\u30027\u6211\u5e08\u7236\u66fe\u7ecf\u8bf4\u8fc7\uff0c\u751f\u547d\uff0c\u6bd4\u4efb\u4f55\u4e1c\u897f\u90fd\u91cd\u8981\u3002\u53ef\u662f\uff0c\u6211\u4eca\u5929\u60f3\u544a\u8bc9\u4ed6\uff0c\u6709\u4e00\u6837\u4e1c\u897f\uff0c\u6bd4\u751f\u547d\u8fd8\u8981\u91cd\u8981\uff0c\u90a3\u5c31\u662f\u2014\u2014\u4fe1\u5ff5\uff018\u6709\u4e9b\u75db\u82e6\uff0c\u82e5\u975e\u4eb2\u8eab\u7ecf\u5386\uff0c\u4f60\u6c38\u8fdc\u65e0\u6cd5\u6e05\u695a\u4e86\u89e3\u5176\u4e2d\u7684\u6ecb\u5473\u30029\u771f\u5b9e\u7684\u4e16\u754c\uff0c\u867d\u7136\u6b8b\u9177\uff0c\u53ef\u662f\u6839\u672c\u65e0\u6cd5\u6539\u53d8\u300210\u6211\u4eec\u73b0\u5728\uff0c\u5df2\u7ecf\u4e0d\u518d\u5f77\u5fa8\uff0c\u6211\u4eec\u5df2\u7ecf\u77e5\u9053\u81ea\u5df1\u7684\u771f\u6b63\u4f7f\u547d\uff0111\u6216\u8bb8\uff0c\u6b7b\uff0c\u4e5f\u662f\u4e00\u79cd\u89e3\u8131\u300212\u6216\u8bb8\uff0c\u7075\u9b42\u7684\u672c\u6027\u5c31\u662f\u55dc\u8840"
}