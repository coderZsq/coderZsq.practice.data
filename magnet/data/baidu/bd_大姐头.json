    "values": [
        "\u4e2d\u6cfd\u65e9\u82d7\u00a0",
        "Nakazawa Sanae\u00a0",
        "\u300a\u8db3\u7403\u5c0f\u5c06\u300b\uff08",
        "\u5929\u4f7f\u4e4b\u7ffc",
        "\uff09",
