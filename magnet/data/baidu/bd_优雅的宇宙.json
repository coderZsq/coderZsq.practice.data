    "values": [
        "\u4f18\u96c5\u7684\u5b87\u5b99",
        "The Elegant Universe",
        "",
        "\u5e03\u8d56\u6069\u00b7\u683c\u6797",
        "",
        "\u00a0",
        "\u5e03\u8d56\u6069\u00b7\u683c\u6797",
