        "\u751f\u7269\u75c5\u6bd2",
        "Biological virus",
        "1\u79cd\u72ec\u7279\u7684\u4f20\u67d3\u56e0\u5b50",
        "\u81ea\u4e3b\u5730\u590d\u5236",
