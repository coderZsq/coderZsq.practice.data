6709\u5357\u821c\u7684\u75d5\u8ff9\u3002\u91d1\u5b87\u5f6c\u8bf4\u5728\u90a3\u573a\u620f\u4e2d\u5357\u821c\u50cf\u5e7b\u5f71\u4e00\u6837\u51fa\u73b0\uff0c\u4ed6\u5728\u4e00\u65c1\u770b\u7740\u674e\u949f\u7855\u6f14\u620f\u5c31\u5df2\u7ecf\u6cea\u6d41\u6ee1\u9762\u00a0\u3002\u300a\u5b66\u68212013\u300b\u5145\u6ee1\u70ed\u6cea\u7684\u6bd5\u4e1a\u5178\u793c\u300a\u5b66\u68212013\u300b\u6700\u540e\u7684\u62cd\u6444\u73b0\u573a\u5f15\u8d77\u4e86\u5927\u5bb6\u7684\u70ed\u6cea\u3002\u5982\u540c\u771f\u6b63\u6bd5\u4e1a\u5178\u793c\u4e00\u6837\uff0c\u5728\u5267\u4e2d\u7684\u6700\u540e\u6bd5\u4e1a\u5178\u793c\u4e0a\u5bfc\u6f14\u674e\u6c11\u6d2a\u4eb2\u81ea\u4e3a2\u5e742\u73ed\u7684\u5b66\u751f\u4eec\u4e00\u4e00\u6388\u4e88\u4e86\u8bc1\u4e66\u00a0\u3002\u300a\u5b66\u68212013\u300b2\u5e742\u73ed\u771f\u6b63\u7684\u6700\u540e\u4e00\u8bfe\u300a\u5b66\u68212013\u300b\u901a\u8fc7\u7279\u8f91\u300a\u5b66\u68212013\uff0c\u53bb\u5b66\u6821\u5427\u300b\u4e3a\u89c2\u4f17\u8868\u793a\u611f\u8c22\uff0c\u4ece\u6f14\u5458\u4eec\u7684\u5f62\u8c61\u5230\u5b66\u6821\u7684\u5386\u53f2\u8fdb\u884c\u4e86\u603b\u7ed3\u3002\u300a\u5b66\u68212013\u300b2\u5e742\u73ed\u7684\u6240\u6709\u540c\u5b66\u5168\u90e8\u51fa\u5e2d\u00a0\u3002\u5b66\u6821\u4f01\u5212(9\u5f20)\u300a\u5b66\u68212013\u300b\u4e2d\u5b66\u751f\u5438\u70df\u3001\u9738\u51cc\u4ee5\u53ca\u5b66\u751f\u95f4\u770b\u4e0d\u89c1\u5374\u7740\u5b9e\u5b58\u5728\u7684\u201c\u6743\u5229\u6784\u9020\u201d\u7684\u6545\u4e8b\uff0c\u8ba9\u89c2\u4f17\u91cd\u65b0\u5ba1\u89c6\u5f53\u4e0b\u5b66\u6821\u3002\u800c\u6700\u5f15\u4eba\u6ce8\u76ee\u7684\u5219\u5c5e\u4e2a\u6027\u5341\u8db3\u7684\u4eba\u7269\u89d2\u8272\uff0c\u9ad8\u5357\u821c\u767d\u5929\u4e0a\u8bfe\u665a\u4e0a\u5e2e\u4eba\u8dd1\u817f\uff0c\u7a00\u91cc\u7cca\u6d82\u5730\u5ea6\u8fc7\u6bcf\u4e00\u5929\uff0c\u662f\u4e2a\u6beb\u65e0\u6b32\u671b\u3001\u6ca1\u6709\u76ee\u6807\u3001\u6ca1\u6709\u7406\u60f3\u7684\u5b66\u751f\uff0c\u4e3a\u4e86\u6c42\u5f97\u4e00\u5f20\u6bd5\u4e1a\u8bc1\u53ea\u5f97\u5929\u5929\u6765\u4e0a\u5b66\uff0c\u6dcb\u6f13\u5c3d\u81f4\u5730\u523b\u753b\u51fa\u5f53\u4eca\u793e\u4f1a\u9752\u5c11\u5e74\u7684\u5fc3\u7406\u00a0\u3002\uff08\u97e9\u661f\u7f51\u8bc4\uff09\u300a\u5b66\u68212013\u300b\u66f4\u52a0\u771f\u5b9e\u7684\u8d34\u8fd1\u4e8e\u73b0\u5b9e\u6821\u56ed\u751f\u6d3b\u3002\u5728\u5267\u4e2d\u51fa\u73b0\u5173\u4e8e\u53cb\u60c5\u3001\u68a6\u60f3\u548c\u4eba\u751f\u4e0d\u540c\u5c42\u9762\u4e0a\u7684\u63a2\u8ba8\uff0c\u8be5\u5267\u64ad\u51fa\u540e\u5438\u5f15\u5230\u4e0d\u540c\u5e74\u9f84\u6bb5\u89c2\u4f17\u7684\u559c\u7231\uff0c\u5b66\u751f\u65cf\u7fa4\u8ba4\u4e3a\u5267\u4e2d\u63cf\u51992\u5e742\u73ed\u7684\u5b66\u751f\u4eec\u6240\u9762\u4e34\u7684\u95ee\u9898\u6b63\u662f\u81ea\u5df1\u6240\u7ecf\u5386\u7684\u6545\u4e8b\uff0c\u5347\u5b66\u538b\u529b\u3001\u8001\u5e08\u5bf9\u5f85\u4f18\u5dee\u751f\u7684\u4e0d\u540c\u6001\u5ea6\u3001\u6821\u56ed\u66b4\u529b\u3001\u8ddf\u73ed\u5c0f\u55bd\u7f57\u3001\u7236\u6bcd\u5f3a\u5236\u6559\u80b2\u5f15\u53d1\u81ea\u6740\u7b49\u4e8b\u4ef6\uff0c\u5bb6\u957f\u4eec\u5728\u6536\u770b\u540e\u5219\u611f\u53f9\u5267\u4e2d\u63cf\u5199\u7684\u5b66\u751f\u5bb6\u957f\u4e0e\u81ea\u5df1\u6709\u7740\u76f8\u4f3c\u7684\u5730\u65b9\uff0c\u800c\u6b63\u5728\u4ece\u4e8b\u6559\u80b2\u884c\u4e1a\u7684\u6559\u5e08\u4eec\u591a\u4f1a\u8ba4\u4e3a\u8be5\u5267\u5982\u5b9e\u7684\u53cd\u6620\u4e86\u8001\u5e08\u5728\u6559\u80b2\u5b66\u751f\u4e2d\u7684\u9762\u4e34\u7684\u56f0\u96be\u5904\u5883\u3002\u300a\u5b66\u68212013\u300b\u6b63\u662f\u4e00\u90e8\u80fd\u5f15\u8d77\u4e0d\u540c\u9636\u5c42\u89c2\u4f17\u5171\u9e23\u7684\u7535\u89c6\u5267\u00a0\u3002\uff08\u65b0\u6d6a\u7f51\u8bc4\uff09\u4e0d\u7ba1\u51fa\u6f14\u4ec0\u4e48\u6837\u7684\u4f5c\u54c1\uff0c\u674e\u949f\u7855\u90fd\u80fd\u5f88\u597d\u5730\u7406\u89e3\u5176\u5185\u5bb9\uff0c\u5c06\u89d2\u8272\u201c\u674e\u949f\u7855\u5316\u201d\u5e76\u5b8c\u7f8e\u5730\u6f14\u7ece\u7ed9\u5927\u4f17\u3002\u8fd9\u6837\u70ed\u8840\u6295\u5165\u8fdb\u6f14\u6280\u7684\u4ed6\uff0c\u4e0d\u5f97\u4e0d\u8ba9\u4eba\u4eec\u559c\u7231\u3002\u5728KBS\u7535\u89c6\u5267\u300a\u5b66\u68212013\u300b\u4e2d\uff0c\u674e\u949f\u7855\u9970\u6f14\u4e86\u5185\u5fc3\u85cf\u6709\u79d8\u5bc6\u3001\u7559\u7ea7\u5e76\u6210\u4e3a2\u5e74\u7ea72\u73ed\u73ed\u957f\u7684\u9ad8\u5357\u821c\u3002\u9ad8\u5357\u821c\u9690\u7792\u4e86\u81ea\u5df1\u64c5\u957f\u6253\u67b6\u7684\u4e8b\uff0c\u662f\u4e2a\u5c31\u7b97\u88ab\u6253\u4e5f\u4f1a\u7ad9\u5728\u5f31\u5c0f\u670b\u53cb\u4e00\u65b9\u7684\u201c\u6b63\u4e49\u201d\u4eba\u7269\u3002\u6b63\u662f\u56e0\u4e3a\u674e\u949f\u7855\u5bf9\u89d2\u8272\u8be0\u91ca\u900f\u5f7b\uff0c\u89c2\u4f17\u4eec\u624d\u901a\u8fc7\u4ed6\u770b\u5230\u4e2a\u771f\u5b9e\u7684\u9ad8\u5357\u821c\u3002\u5728\u5c06\u6821\u56ed\u4e2d\u9ed1\u6697\u4e00\u9762\u771f\u5b9e\u53cd\u6620\u51fa\u6765\u7684\u300a\u5b66\u68212013\u300b\u4e2d\uff0c\u674e\u949f\u7855\u50cf\u4e00\u675f\u201c\u91ce\u82b1\u201d\u822c\u5b58\u5728\u7740\u3002\u201c\u7ec6\u770b\u65b9\u77e5\u7f8e\u4e3d\uff0c\u4e45\u54c1\u624d\u89e3\u53ef\u7231\u201d\uff0c\u5982\u540c\u8fd9\u53e5\u8bdd\u4e00\u6837\uff0c\u674e\u949f\u7855\u5b8c\u7f8e\u5730\u6210\u4e3a\u9ad8\u5357\u821c\uff0c\u5c06\u8fd9\u4e2a\u5750\u5728\u6559\u5ba4\u89d2\u843d\u7684\u4eba\u7269\u751f\u52a8\u5730\u8868\u73b0\u51fa\u6765\u00a0\u3002\uff08bnt\u65b0\u95fb\u7f51\u8bc4\uff09"
}