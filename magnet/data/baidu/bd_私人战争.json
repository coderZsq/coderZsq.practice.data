        "\u79c1\u4eba\u6218\u4e89",
        "A Private War\u3001Marie Colvin",
        "\u4e00\u4e2a\u4eba\u7684\u6218\u4e89\u3001\u7b2c\u4e00\u773c\u6230\u7dda\uff08\u6e2f\uff09",
