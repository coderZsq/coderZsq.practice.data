06f\u53f030\u3086\u3063\u304f\u308a\u6b69\u304d\u51fa\u305931\u706f\u53f0232\u706f\u53f0\u306e\u30d0\u30e9\u30fc\u30c9\u53d1\u5e03\u65f6\u95f4\uff1a2013\u5e7412\u670825\u65e5\u6f6e\u9a12\u306e\u30e1\u30e2\u30ea\u30fc\u53d1\u884c\u65f6\u95f4\uff1a2013\u5e747\u670831\u65e5\u6f14\u5531\u8005\uff1a \u5929\u91ce\u6625\u5b50\uff08\u5c0f\u6cc9\u4eca\u65e5\u5b50\uff09\u4f5c\u8bcd\uff1a \u5bab\u85e4\u5b98\u4e5d\u90ce\u4f5c\u66f2\uff1a \u5927\u53cb\u826f\u82f1\u3001Sachiko M\u53d1\u884c\u65f6\u95f4\uff1a2013\u5e747\u67083\u65e5\u6f14\u5531\u8005\uff1a\u963f\u7f8e\u6a2a\u753a\u5973\u5b50\u5b66\u56ed\uff08babyraids\u3001\u6c34\u702c\u3044\u306e\u308a\uff09\u5267\u4e2d\u6b4c\u66f2\u5408\u96c6\uff08\u300a\u3042\u307e\u3061\u3083\u3093 \u6b4c\u306e\u30a2\u30eb\u30d0\u30e0 \u522b\u518c\u300b\uff09\u6f6e\u9a12\u306e\u30e1\u30e2\u30ea\u30fc\u6f14\u5531\u8005\uff1a\u5929\u91ce\u6625\u5b50\uff08\u5c0f\u6cc9\u4eca\u65e5\u5b50\uff09\u4f5c\u8bcd\uff1a\u5bab\u85e4\u5b98\u4e5d\u90ce\u4f5c\u66f2\uff1a\u5927\u53cb\u826f\u82f1\u3001Sachiko M\u66a6\u306e\u4e0a\u3067\u306f\u30c7\u30a3\u30bb\u30f3\u30d0\u30fc\u6f14\u5531\u8005\uff1a\u30a2\u30e1\u6a2a\u5973\u5b66\u56ed\u82b8\u80fd\u30b3\u30fc\u30b9\u4f5c\u8bcd\uff1a\u5bab\u85e4\u5b98\u4e5d\u90ce\u4f5c\u66f2\uff1a\u5927\u53cb\u826f\u82f1\u3001Sachiko M\u3001\u6c5f\u85e4\u76f4\u5b50\u3001\u9ad8\u4e95\u5eb7\u751f\u5730\u5143\u306b\u5e30\u308d\u3046\u6f14\u5531\u8005\uff1aGMT\u4f5c\u8bcd\uff1a \u5bab\u85e4\u5b98\u4e5d\u90ce\u4f5c\u66f2\uff1a Sachiko M\u3001\u5927\u53cb\u826f\u82f1\u3044\u3064\u3067\u3082\u68a6\u3092\uff08\u300a\u6c38\u6000\u68a6\u60f3\u300b\uff09\u6f14\u5531\u8005\uff1a\u6865\u5e78\u592b\u3001\u5409\u6c38\u5c0f\u767e\u5408\u4f5c\u8bcd\uff1a\u4f50\u4f2f\u5b5d\u592b\u4f5c\u66f2\uff1a\u5409\u7530\u6b63\u5357\u90e8\u30c0\u30a4\u30d0\u30fc \uff08\u6559\u5ba4\u7248\uff09\u6f14\u5531\u8005\uff1a\u77f6\u91ce\u5148\u751f\u3001 \u5317\u4e09\u9646\u9ad8\u6821\u6f5c\u6c34\u571f\u6728\u79d1\u751f\u5f92\u305f\u3061\u4f5c\u8bcd\uff1a\u5b89\u85e4\u7766\u592b\u4f5c\u66f2\uff1a\u5b89\u85e4\u7766\u592b\u6f6e\u9a12\u306e\u30e1\u30e2\u30ea\u30fc\uff08\u592a\u5dfb\u30c7\u30e2\u30d0\u30fc\u30b8\u30e7\u30f3\uff09\u6f14\u5531\u8005\uff1a\u8352\u5dfb\u592a\u4e00\uff08\u53e4\u7530\u65b0\u592a\uff09\u5730\u5143\u306b\u5e30\u308d\u3046\uff08\u592a\u5dfb\u30c7\u30e2\u30d0\u30fc\u30b8\u30e7\u30f3\uff09\u6f14\u5531\u8005\uff1a\u8352\u5dfb\u592a\u4e00\uff08\u53e4\u7530\u65b0\u592a\uff09\u3044\u3089\u306a\u3044\u30d0\u30a4\u30af\u4e70\u3044\u53d6\u308b\u305e\u3046!\u6f14\u5531\u8005\uff1a\u65e5\u672cACG\u6f6e\u9a12\u306e\u30e1\u30e2\u30ea\u30fc \uff08\u5bb4\u4f1a\u5217\u8f66\u7248\uff09\u6f14\u5531\u8005\uff1a\u6865\u672c\u7231\u3001\u80fd\u5e74\u73b2\u5948\u4f5c\u8bcd\uff1a\u5bab\u85e4\u5b98\u4e5d\u90ce\u4f5c\u66f2\uff1a\u5927\u53cb\u826f\u82f1\u3001Sachiko M\u6f6e\u9a12\u306e\u30e1\u30e2\u30ea\u30fc\uff08\u79d8\u8535\u30c7\u30e2\uff09\u6f14\u5531\u8005\uff1aSachiko M\u4f5c\u8bcd\uff1a\u5bab\u85e4\u5b98\u4e5d\u90ce\u4f5c\u66f2\uff1a\u5927\u53cb\u826f\u82f1\u3001Sachiko M\u53d1\u884c\u65f6\u95f4\uff1a 2013\u5e7408\u670828\u65e5\u97f3\u4e50\u539f\u58f0[8-13]\u00a01\u4e3b\u89d2\u80fd\u5e74\u73b2\u5948\u5728\u9009\u89d2\u4f1a\u4e2d\u4ece1,953\u540d\u62a5\u540d\u8005\u4e2d\u8131\u9896\u800c\u51fa\u3002\u867d\u7136\u6668\u95f4\u5267\u6709\u901a\u8fc7\u8bd5\u955c\u9009\u51fa\u5973\u4e3b\u89d2\u7684\u4f20\u7edf\uff0c\u4f462010\u5e74\u4ee5\u6765\u5728\u4e1c\u4eac\u5de5\u4f5c\u5ba4\u5236\u4f5c\u7684\u6bcf\u5e74\u524d\u671f\uff084\u6708-9\u6708\uff09\u4f5c\u54c1\u90fd\u672a\u4e3e\u884c\u7504\u9009\u4f1a\uff0c\u56e0\u6b64\u300a\u6d77\u5973\u300b\u91cd\u65b0\u8fdb\u884c\u9009\u62d4\uff0c\u88ab\u5916\u754c\u89c6\u4e3a\u56de\u5f52\u6668\u95f4\u5267\u539f\u70b9\u3001\u4ee5\u65b0\u7684\u53d1\u5c55\u4e3a\u76ee\u6807\u4e4b\u4e3e\u3002\u00a02\u5929\u91ce\u6625\u5b50\u7684\u626e\u6f14\u8005\u5c0f\u6cc9\u4eca\u65e5\u5b50\u6f14\u5531\u7684\u63d2\u66f2\u300a\u6d77\u6f6e\u7684memory\u300b\u56e0\u6000\u65e7\u768480\u5e74\u4ee3\u66f2\u98ce\u548c\u5bab\u85e4\u5b98\u4e5d\u90ce\u521b\u4f5c\u7684\u6b4c\u8bcd\u53d7\u5230\u89c2\u4f17\u559c\u7231\uff0c\u4ee5\u89d2\u8272\u7684\u540d\u5b57\u63a8\u51fa\u7684\u5355\u66f2CD\u6253\u5165ORICON\u516c\u4fe1\u699c\u7684\u5355\u66f2\u699c\u7b2c\u4e8c\u540d\uff0c\u8fd9\u662f\u5979\u668c\u8fdd20\u5e74\u518d\u6b21\u6253\u5165ORICON\u7684\u524d\u4e09\u3002\u00a0[15-16]\u00a0\u300a\u6d77\u5973\u300b32013\u5e749\u6708\uff0c\u7531\u80fd\u5e74\u73b2\u5948\u3001\u5c0f\u6cc9\u4eca\u65e5\u5b50\u3001\u6865\u672c\u7231\u7b49\u4eba\u6f14\u5531\u7684\u5267\u4e2d\u6b4c\u66f2\u4e13\u8f91\u300a\u6d77\u5973\u97f3\u4e50\u4e13\u8f91\u300b\u6210\u4e3aORICON\u699c\u5355\u51a0\u519b\uff0c\u8fd9\u662fNHK\u6668\u95f4\u5267\u53f2\u4e0a\u7684\u7b2c\u4e00\u6b21\u3002\u00a04\u3002\u8be5\u5267\u7b2c\u4e8c\u672c\u89c2\u770b\u6307\u5357\u53d1\u884c\u9996\u5468\u7684\u9500\u91cf\u5728Oricon\u4e66\u7c4d\u6392\u884c\u699c\u7684\u7535\u89c6\u8282\u76ee\u76f8\u5173\u90e8\u95e8\u4e0a\u53d6\u5f97\u7b2c\u4e00\u540d\u3002\u8fd9\u662f\u9996\u6b21\u6709\u89c2\u770b\u6307\u5357\u53d6\u5f97\u8be5\u699c\u7b2c\u4e00\u540d\u3002\u00a05\u8be5\u5267\u64ad\u51fa\u540e\uff0c\u5927\u6279\u89c2\u5149\u5ba2\u9020\u8bbf\u5916\u666f\u62cd\u6444\u5730\u5ca9\u624b\u53bf\u4e45\u6148\u5e02\u5c0f\u8896\u6d77\u5cb8\u7684\u201c\u6d77\u5973\u4e2d\u5fc3\u201d\uff0c2013\u5e747~8\u6708\u4e45\u6148\u5e02\u7684\u6e38\u5ba2\u6570\u91cf\u662f\u53bb\u5e74\u540c\u671f\u768423\u500d\uff0c\u5267\u4e2d\u51fa\u73b0\u8fc7\u7684\u5f53\u5730\u5546\u54c1\u4ea6\u9500\u60c5\u751a\u4f73\uff0c[19-20]\u00a0\u56e0\u6155\u540d\u800c\u6765\u7684\u6e38\u5ba2\u592a\u591a\uff0c\u6d77\u5973\u4e2d\u5fc3\u4e00\u5ea6\u5b9e\u65bd\u4e86\u8f66\u8f86\u7ba1\u5236\u3002\u00a0\u5ca9\u624b\u94f6\u884c\u667a\u5e93\u5ca9\u624b\u7ecf\u6d4e\u7814\u7a76\u6240\u66fe\u4f30\u7b97\uff0c2013\u5e74\u300a\u6d77\u5973\u300b\u5e26\u6765\u7684\u7ecf\u6d4e\u6548\u76ca\u53ef\u80fd\u8fbe32\u4ebf8400\u4e07\u65e5\u5143\u3002\u00a06\u8be5\u5267\u4e2d\u7684\u53f0\u8bcd\u3001\u5ca9\u624b\u53bf\u65b9\u8a00\u201cJIEJIEJIE\u201d\u6210\u4e3a\u5f53\u5e74\u7684\u65e5\u672c\u6d41\u884c\u8bed\u3002\u00a07\u300a\u6d77\u5973\u300b\u5b8c\u7ed3\u540e\uff0c\u6709\u89c2\u4f17\u56e0\u6b64\u60c5\u7eea\u4f4e\u843d\uff0c\u88ab\u79f0\u4e3a\u201c\u6d77\u5973\u5931\u843d\u75c7\u5019\u7fa4\u201d\u3002\u00a08\u5267\u4e2d\u5c0f\u79cb\u7684\u7236\u4eb2\u4f7f\u7528\u7684\u51fa\u79df\u8f66\u662f\u97e9\u56fd\u8f66\uff0c\u62db\u81f4\u89c2\u4f17\u7684\u4e0d\u6ee1\u3002\u00a0\u4e1c\u4eac\u56fd\u9645\u7535\u89c6\u8282\u201c\u4e1c\u4eac\u7535\u89c6\u5267\u5956\u201d\u83b7\u5956\u8bb0\u5f55[26-28]\u00a0\u62cd\u6444\u8fc7\u7a0b2012\u5e746\u67084\u65e5\uff0c\u65e5\u672c\u653e\u9001\u534f\u4f1a\uff08NHK\uff09\u5728\u4e3e\u529e\u8bb0\u8005\u62db\u5f85\u4f1a\uff0c\u516c\u5e03\u5236\u4f5c\u7b2c88\u90e8\u6668\u95f4\u5c0f\u8bf4\u8fde\u7eed\u5267\u300a\u6d77\u5973\u300b\u7684\u6d88\u606f\uff0c\u9664\u4e86\u6545\u4e8b\u5927\u7eb2\u5916\uff0c\u4ea6\u516c\u5e03\u7f16\u5267\u4e3a\u5bab\u85e4\u5b98\u4e5d\u90ce\u3002\u8fd9\u662f\u5bab\u85e4\u5b98\u4e5d\u90ce\u9996\u6b21\u64b0\u5199NHK\u7684\u7535\u89c6\u5267\u5267\u672c\u3002\u00a0\u540c\u5e747\u670826\u65e5\uff0cNHK\u516c\u5e03\u5973\u4e3b\u89d2\u5929\u91ce\u79cb\u4eba\u9009\u4e3a\u521d\u6b21\u4e3b\u6f14\u7535\u89c6\u5267\u7684\u80fd\u5e74\u73b2\u5948\u3002\u00a0\u300a\u6d77\u5973\u300b\u75312012\u5e7410\u670818\u65e5\u8d77\u5728\u5ca9\u624b\u53bf\u4e45\u6148\u5e02\u5f00\u59cb\u62cd\u6444\uff0c\u5728\u7fcc\u5e74\uff082013\u5e74\uff098\u67081\u65e5\u6740\u9752\uff0c\u62cd\u6444\u65f6\u671f\u7ea610\u4e2a\u6708\u3002\u00a0\u521b\u4f5c\u80cc\u666f\u8be5\u5267\u65e5\u6587\u540d\u79f0\u201c\u3042\u307e\u3061\u3083\u3093\u201d\u9664\u4e86\u201c\u6d77\u5973\u201d\uff08\u6d77\u5973\u3061\u3083\u3093\uff09\u7684\u610f\u601d\u4e4b\u5916\uff0c\u8fd8\u6709\u201c\u4e0d\u6210\u719f\u7684\u5c0f\u5b69\u5b50\uff08\u7518\u3061\u3083\u3093\uff09\u6210\u957f\u8d77\u6765\u201d\u7684\u610f\u601d\u3002\u7f16\u5267\u5bab\u85e4\u5b98\u4e5d\u90ce\u60f3\u63cf\u5199\u201c\u5076\u50cf\u201d\u632f\u5174\u4e61\u6751\u7ecf\u6d4e\u4e3a\u6838\u5fc3\u7684\u6545\u4e8b\uff0c\u4e00\u4e2a\u504f\u50fb\u5c71\u6751\u7684\u5c11\u5973\u6210\u957f\u4e3a\u5076\u50cf\uff0c\u7c89\u4e1d\u4e0d\u8fdc\u5343\u91cc\u800c\u6765\u53ea\u4e3a\u770b\u5979\u4e00\u773c\uff0c\u4ece\u800c\u5728\u7cbe\u795e\u548c\u7269\u8d28\u4e24\u65b9\u9762\u4f7f\u4e61\u6751\u590d\u5174\u3002\u00a02011\u5e7411\u6708\uff0c\u5728\u8fd8\u6ca1\u51b3\u5b9a\u6545\u4e8b\u53d1\u751f\u7684\u821e\u53f0\u4e4b\u65f6\uff0c\u5bab\u85e4\u5b98\u4e5d\u90ce\u5230\u6bcd\u4eb2\u7684\u6545\u4e61\u5ca9\u624b\u53bf\u4e45\u6148\u5e02\u8003\u5bdf\uff0c\u5f97\u77e5\u4e86\u5f53\u5730\u6d77\u5973\u3001\u5730\u533a\u94c1\u8def\uff08\u4e09\u9646\u94c1\u9053\u5317\u8c37\u6e7e\u7ebf\uff09\u3001\u7425\u73c0\u7b49\u4eba\u4e8b\u7269\u7684\u5b58\u5728\uff0c\u4ed6\u4ece\u4e2d\u83b7\u5f97\u7075\u611f\uff0c\u4fbf\u51b3\u5b9a\u4ee5\u8be5\u5730\u4e3a\u6545\u4e8b\u821e\u53f0\u3002\u00a02013\u5e744\u67081\u65e5\u81f39\u670828\u65e5\u6bcf\u5468\u4e00\u81f3\u5468\u516d8:00\uff5e8:15\u524d\u6863\u8282\u76ee\uff1a\u7eaf\u4e0e\u7231\u63a5\u6863\u8282\u76ee\uff1a\u591a\u8c22\u6b3e\u5f852013\u5e7411\u670811\u65e5\u81f32014\u5e741\u670821\u65e5\u6bcf\u5468\u4e00\u81f3\u5468\u4e94 20:00~22:002014\u5e747\u67086\u65e5\u81f32015\u5e741\u670824\u65e5\u6bcf\u5468\u516d\u3001\u5468\u65e518\uff1a05~19\uff1a002015\u5e744\u67086\u65e5~10\u67083\u65e5\u6bcf\u5468\u4e00\u81f3\u5468\u516d7\uff1a15~7\uff1a30\u57fa\u672c\u4fe1\u606f\u00a0\u00a0\u00a0\u64ad\u51fa\u65e5\u671f\uff082013\u5e74\uff09\u304a\u3089\u3001\u3053\u306e\u6d77\u304c\u597d\u304d\u3060!\u304a\u3089\u3001\u4e1c\u4eac\u3055\u5e30\u308a\u305f\u304f\u306d\u3047\u5e73\u5747\u6536\u89c6\u7387\uff1a206%\u6536\u89c6\u7387[32-33]\u00a0\u300a\u6d77\u5973\u300b\u7b2c\u4e00\u96c6\u77ed\u77ed15\u5206\u949f\uff0c\u5374\u5df2\u7ecf\u96d5\u7422\u51fa\u4e86\u6545\u4e8b\u7684\u9aa8\u9abc\uff0c\u4e00\u5f00\u59cb\u5c31\u628a\u6545\u4e8b\u7684\u6784\u9020\u548c\u7740\u529b\u70b9\u6e05\u6670\u5448\u73b0\uff0c\u4ee4\u89c2\u4f17\u7981\u4e0d\u4f4f\u671f\u5f85\u4e0b\u9762\u7684\u5267\u60c5\u3002\uff08\u7f51\u6613\u5a31\u4e50\u8bc4\u8bba\uff09\u00a0\u6668\u95f4\u5267\u300a\u6d77\u5973\u300b\u867d\u662f\u975e\u5e38\u666e\u901a\u7684\u5076\u50cf\u52a0\u52b1\u5fd7\u9898\u6750\uff0c\u4f46\u9009\u62e9\u6d77\u5973\u8fd9\u4e2a\u804c\u4e1a\u4f5c\u4e3a\u5207\u5165\u70b9\uff0c\u8ba9\u6574\u4e2a\u5267\u96c6\u53d8\u5f97\u7279\u522b\u8d77\u6765\u3002\u5267\u4e2d\u5145\u5206\u5229\u7528\u4e86\u6d77\u5973\u8fd9\u4e00\u804c\u4e1a\u7684\u89c2\u8d4f\u6027\u548c\u6355\u635e\u65f6\u95f4\u4e0a\u53d7\u9650\u7684\u7279\u70b9\uff0c\u5236\u9020\u51fa\u5404\u79cd\u8d77\u627f\u8f6c\u5408\uff0c\u8fbe\u5230\u610f\u60f3\u4e0d\u5230\u7684\u6548\u679c\u3002\u9664\u4e86\u7cbe\u5f69\u7684\u5267\u60c5\uff0c\u8f7b\u677e\u6109\u60a6\u7684\u89c2\u8d4f\u4f53\u9a8c\u4ee5\u53ca\u6e29\u99a8\u7684\u5bb6\u5ead\u6c1b\u56f4\uff0c\u7ed9\u6709\u751f\u6d3b\u538b\u529b\u7684\u89c2\u4f17\u5e26\u53bb\u65e0\u9650\u7684\u6b63\u80fd\u91cf\uff0c\u662f\u8be5\u5267\u53d7\u89c2\u4f17\u9752\u7750\u7684\u539f\u56e0\u4e4b\u4e00\u3002\u5973\u4e3b\u89d2\u7684\u7cbe\u5f69\u8868\u6f14\u4e5f\u662f\u6b64\u5267\u5927\u53d7\u6b22\u8fce\u7684\u91cd\u8981\u56e0\u7d20\u3002\u80fd\u5e74\u73b2\u5948\u5851\u9020\u7684\u5f00\u6717\u3001\u6d3b\u6cfc\u53c8\u65f6\u800c\u6ca1\u5934\u8111\u7684\u6d77\u5973\u5f62\u8c61\u6df1\u5165\u4eba\u5fc3\uff0c\u5979\u7684\u7b11\u5bb9\u66f4\u6210\u4e3a\u6cbb\u6108\u89c2\u4f17\u7684\u826f\u836f\u3002\u6b64\u5916\uff0c\u6865\u672c\u7231\u3001\u6709\u6751\u67b6\u7eaf\u9970\u7cbe\u7f8e\u7684\u9762\u5bb9\u4e5f\u8ba9\u89c2\u4f17\u767e\u770b\u4e0d\u538c\u3002\u6b64\u5916\uff0c\u5267\u4e2d\u6e5b\u84dd\u7684\u5927\u6d77\u3001\u8d28\u6734\u7684\u4e61\u6751\u98ce\u8c8c\u3001\u4eba\u70df\u7a00\u5c11\u7684\u7834\u65e7\u706b\u8f66\u7ad9\u3001\u8bf1\u4eba\u7684\u6d77\u80c6\u7b49\uff0c\u52a0\u4e0a\u8fdc\u79bb\u57ce\u5e02\u7684\u60a0\u95f2\u751f\u6d3b\uff0c\u65e0\u4e00\u4e0d\u8bf1\u60d1\u7740\u90fd\u5e02\u91cc\u7684\u7537\u7537\u5973\u5973\u3002\u00a0\uff08\u5916\u6ee9\u753b\u62a5\u8bc4\u4ef7\uff09\u7f16\u5267\u5bab\u85e4\u5b98\u4e5d\u90ce\u628a\u5168\u5267\u5185\u5bb9\u4e00\u5206\u4e3a\u4e8c\uff0c\u4ee5\u4fbf\u540c\u65f6\u54cd\u5e942011\u5e74\u4e1c\u65e5\u672c\u5927\u5730\u9707\u540e\u7684\u65f6\u4ee3\u6c1b\u56f4\uff1a\u524d\u534a\u90e8\u91cc\u5c0f\u9547\u7528\u5c3d\u65b9\u6cd5\u590d\u5174\uff0c\u6b63\u662f\u4e1c\u65e5\u672c\u5730\u9707\u540e\u7684\u4e3b\u65cb\u5f8b\uff0c\u540e\u534a\u90e8\u5c0f\u79cb\u7684\u5076\u50cf\u4e4b\u8def\u6a21\u4eff\u81eaAKB48\uff0c\u800cAKB48\u6b63\u662f\u65e5\u672c\u4e0a\u4e0b\u529b\u6367\u7684\u5f62\u8c61\u5de5\u7a0b\u3002\u4f46\u662f\u300a\u6d77\u5973\u300b\u8868\u9762\u4e0a\u8ba8\u597d\u7684\u5267\u60c5\u6846\u67b6\u8bbe\u5b9a\u4e0d\u8fc7\u662f\u5e4c\u5b50\uff0c\u5bfb\u68a6\u7684\u80cc\u540e\u5145\u6ee1\u8840\u8ff9\u6591\u6591\u7684\u75db\u82e6\u56de\u5fc6\uff0c\u5c0f\u79cb\u7684\u660e\u661f\u68a6\u4e0d\u8fc7\u662f\u6bcd\u4eb2\u5c0f\u6625\u7684\u5f53\u5e74\u7684\u7ffb\u7248\uff0c\u800cGMT\u4e4b\u68a6\u539f\u6765\u53ef\u4ee5\u513f\u620f\u5230\u56e0\u4e2a\u4eba\u6069\u6028\u800c\u68a6\u9192\uff0c\u5267\u96c6\u7684\u5185\u6db5\u6b63\u662f\u5bab\u85e4\u4e00\u76f4\u5728\u5176\u4f5c\u54c1\u4e2d\u5f3a\u8c03\u7684\u90a3\u79cd\u5f25\u6f2b\u5728\u65e5\u5e38\u751f\u6d3b\u4e2d\u7684\u7834\u706d\u6c14\u606f\u3002\u00a0\uff08\u7b2c\u4e00\u8d22\u7ecf\u8bc4\u8bba\uff09\u901a\u8fc7\u8d34\u8fd1\u751f\u6d3b\u7684\u5973\u4e3b\u4eba\u516c\u5f62\u8c61\uff0c\u5bab\u85e4\u5b98\u4e5d\u90ce\u5448\u73b0\u4e86\u201c\u5f7b\u5e95\u80af\u5b9a\u73b0\u5b9e\u5341\u5206\u91cd\u8981\u201d\u7684\u4e16\u754c\u89c2\u3002\u4f8b\u5982\uff0c\u66fe\u611f\u5230\u65e0\u671b\u7684\u5317\u4e09\u9646\u7684\u4eba\u4eec\u91cd\u65b0\u8ba4\u8bc6\u5230\u5bb6\u4e61\u7684\u4ef7\u503c\uff0c\u4ece\u800c\u81f4\u529b\u4e8e\u632f\u5174\u7ecf\u6d4e\uff1b\u66fe\u7ecf\u6f20\u89c6\u5bb6\u4e61\u548c\u6bcd\u4eb2\u7684\u6625\u5b50\u5728\u5c0f\u79cb\u7684\u5f71\u54cd\u4e0b\u6539\u53d8\u4e86\u60f3\u6cd5\uff1b\u7ed3\u8863\u542c\u4e86\u597d\u53cb\u5c0f\u79cb\u8fd4\u4e61\u7684\u7406\u7531\u540e\u51b3\u5b9a\u7559\u5728\u5bb6\u4e61\uff0c\u5bfb\u627e\u81ea\u5df1\u80fd\u505a\u7684\u4e8b\u60c5\uff1b\u4e3b\u4eba\u516c\u5c0f\u79cb\u81ea\u5df1\u5728\u4eba\u751f\u76ee\u6807\u51e0\u7ecf\u6539\u53d8\u540e\u6700\u7ec8\u8fd8\u662f\u89c9\u5f97\u201c\u5bb6\u4e61\u201d\u597d\u3002\u8fd9\u79cd\u201c\u80af\u5b9a\u201d\u5c31\u662f\u5728\u73b0\u5b9e\u4e2d\u81ea\u7136\u800c\u7136\u5730\u4fc3\u8fdb\u53d8\u9769\u7684\u529b\u91cf\u3002\u6709\u8fc7\u8bf8\u591a\u5931\u8d25\u3001\u4e3a\u5404\u79cd\u4e8b\u60c5\u70e6\u607c\u7684\u5267\u4e2d\u4eba\u7269\u5728\u548c\u5c0f\u79cb\u76f8\u9047\u540e\uff0c\u90fd\u4f1a\u6539\u53d8\u770b\u95ee\u9898\u7684\u65b9\u5f0f\u3002\u672c\u4ee5\u4e3a\u662f\u60b2\u5267\u7684\u4e1c\u897f\u770b\u4e0a\u53bb\u4e5f\u53d8\u6210\u4e86\u559c\u5267\u6216\u5145\u6ee1\u5e7d\u9ed8\u7684\u5143\u7d20\u3002\u00a0\uff08\u671d\u65e5\u65b0\u95fb\u4e2d\u6587\u7f51\u8bc4\u8bba\uff09"
}