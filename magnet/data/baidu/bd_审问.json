    "values": [
        "\u5ba1\u8baf",
        "Trial",
        "sh\u011bnx\u00f9n",
        "\u53f8\u6cd5\u673a\u5173\u67e5\u95ee\u6709\u5173\u6848\u4ef6\u7684\u4e8b\u5b9e",
        "\u300a",
